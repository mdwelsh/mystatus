<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Mood Survey</h:title>
    <model>
      <submission action="https://uwmystatus.appspot.com/submission" method="form-data-post"/>
      <instance>
        <Mood_Survey id="mood_survey">
          <depression>
            <depressionlabel/>
            <sad/>
            <discouraged/>
            <selfesteem/>
            <inadequate/>
            <satisfaction/>
          </depression>
          <suicide>
            <suicidelabel/>
            <suicidalthoughts/>
            <suicidalurges/>
          </suicide>
          <anxiety>
            <anxietylabel/>
            <anxious/>
            <frightened/>
            <worrying/>
            <tense/>
            <nervous/>
          </anxiety>
          <anger>
            <angerlabel/>
            <frustrated/>
            <annoyed/>
            <resentful/>
            <angry/>
            <irritated/>
          </anger>
          <violence>
            <violencelabel/>
            <violentthoughts/>
            <violenturges/>
          </violence>
          <meta>
            <instanceID/>
          </meta>
        </Mood_Survey>
      </instance>
      <bind nodeset="/Mood_Survey/depression/depressionlabel" type="select1"/>
      <bind nodeset="/Mood_Survey/depression/sad" type="select1"/>
      <bind nodeset="/Mood_Survey/depression/discouraged" type="select1"/>
      <bind nodeset="/Mood_Survey/depression/selfesteem" type="select1"/>
      <bind nodeset="/Mood_Survey/depression/inadequate" type="select1"/>
      <bind nodeset="/Mood_Survey/depression/satisfaction" type="select1"/>
      <bind nodeset="/Mood_Survey/suicide/suicidelabel" type="select1"/>
      <bind nodeset="/Mood_Survey/suicide/suicidalthoughts" type="select1"/>
      <bind nodeset="/Mood_Survey/suicide/suicidalurges" type="select1"/>
      <bind nodeset="/Mood_Survey/anxiety/anxietylabel" type="select1"/>
      <bind nodeset="/Mood_Survey/anxiety/anxious" type="select1"/>
      <bind nodeset="/Mood_Survey/anxiety/frightened" type="select1"/>
      <bind nodeset="/Mood_Survey/anxiety/worrying" type="select1"/>
      <bind nodeset="/Mood_Survey/anxiety/tense" type="select1"/>
      <bind nodeset="/Mood_Survey/anxiety/nervous" type="select1"/>
      <bind nodeset="/Mood_Survey/anger/angerlabel" type="select1"/>
      <bind nodeset="/Mood_Survey/anger/frustrated" type="select1"/>
      <bind nodeset="/Mood_Survey/anger/annoyed" type="select1"/>
      <bind nodeset="/Mood_Survey/anger/resentful" type="select1"/>
      <bind nodeset="/Mood_Survey/anger/angry" type="select1"/>
      <bind nodeset="/Mood_Survey/anger/irritated" type="select1"/>
      <bind nodeset="/Mood_Survey/violence/violencelabel" type="select1"/>
      <bind nodeset="/Mood_Survey/violence/violentthoughts" type="select1"/>
      <bind nodeset="/Mood_Survey/violence/violenturges" type="select1"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/Mood_Survey/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
